{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./routes/"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "plugins": ["node_modules/jsdoc-export-default-interop/dist/index"],
  "opts": {
    "template": "node_modules/docdash"
  },
  "docdash": {
    "sort": false,
    "openGraph": {
      "title": "Documentation of ... service",
      "type": "website"
    },
    "meta": {
      "title": "Documentation for ... service"
    },
    "search": true,
    "menu": {
      "Project Website": {
        "href": "https://github.com/Enkel-Digital",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      }
    }
  }
}
