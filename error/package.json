{
  "name": "vue-error-controller",
  "version": "0.1.0",
  "description": "Vue plugin for a global error controller",
  "main": "./src/index.js",
  "files": [
    "src/"
  ],
  "directories": {
    "example": "example"
  },
  "scripts": {
    "publish": "git stash --include-untracked && npm publish && git stash apply"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enkel-Digital/vue-error-controller.git"
  },
  "keywords": [
    "vue",
    "plugin",
    "vue-plugin",
    "error",
    "controller"
  ],
  "author": "JJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Enkel-Digital/vue-error-controller/issues"
  },
  "homepage": "https://github.com/Enkel-Digital/vue-error-controller#readme",
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write '**/*.js'"
    ],
    "*.{json}": [
      "prettier --write '**/*.json'"
    ]
  }
}
