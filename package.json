{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npx cross-env NODE_ENV=development vue-cli-service serve",
    "build": "npx cross-env NODE_ENV=production vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy-firebase-hosting": "firebase deploy --only hosting",
    "deploy": "npm run build && npm run deploy-firebase-hosting"
  },
  "dependencies": {
    "algoliasearch": "^4.2.0",
    "core-js": "^3.6.5",
    "firebase": "^7.14.3",
    "instantsearch.css": "^7.4.2",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.25.3",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-instantsearch": "^2.7.0",
    "vue-observe-visibility": "^0.4.6",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.27",
    "vuex": "^3.3.0",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-pwa": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint"
    ]
  }
}
